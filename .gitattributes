<51강>


#include <iostream>



void swap(int &a, int &b)
{
	int tmp = a;
	a = b;
	b = tmp;
}

void swap(double &a, double &b)
{
	double tmp = a;
	a = b;
	b = tmp;
}

void swap(int* (&a), int* (&b))
{
	int* tmp = a;
	a = b;
	b = tmp;
}

int main()
{
	int a = 20, b = 30;
	double da = 2.22, db = 3.35;
	int* pa = &a, *pb = &b;

	swap(a, b);
	swap(da, db);
	swap(pa, pb);

	std::cout << "a :" << a << std::endl;
	std::cout << "b :" << b << std::endl;

	std::cout << "da:" << da << std::endl;
	std::cout << "db:" << db << std::endl;

	std::cout << "*pa:" << *pa << std::endl;
	std::cout << "*pb:" << *pb << std::endl;
}

---------------------------------------------

using namespace std;

int inventory[64] = { 0 };
int score = 0;

void getitem(int itemid, int cnt =1, int sc = 0)
{
	inventory[itemid] += cnt;
	score += sc;
}

int main()
{
	getitem(6, 5);
	getitem(3, 2);
	getitem(4);
	getitem(11, 34, 7000);

	cout << score << endl;
	for (int i = 0; i < 16; i++)
	{
		cout << inventory[i] << ' ';
	}
	cout << endl;
}



