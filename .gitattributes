int main()
{
	cout << "닥치시오 우서" << endl;
}

---------------------------------------------

using namespace std;

int main()
{
	int a, b;
	cin >> a >> b;

	cout << a << "+" << b << "=" << a + b << endl;
}

---------------------------------------------

using namespace std;

int main()
{
	string str;

	str = "누가 기침소리를 내었는가";
	cout << str << endl;
}

